{
  "cli": {
    "version": ">= 16.18.1",
    "appVersionSource": "local",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "preview-apk": {
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "credentialsSource": "local"
      },
      "ios": {
        "buildConfiguration": "Release",
        "simulator": false
      }
    },
    "production-ios": {
      "ios": {
        "buildConfiguration": "Release",
        "simulator": false
      },
      "autoIncrement": true
    },
    "production-apk": {
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      }
    },
    "production-aab": {
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "local"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    },
    "production-aab": {
      "android": {
        "track": "production"
      }
    }
  }
}